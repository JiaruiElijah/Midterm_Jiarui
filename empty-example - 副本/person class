function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  
  push();
  translate(200,100);
  noStroke();
  
  fill(21,24,58);//body
  rect(0,70,30,45);
  // rect(170,260,60,60);
  rect(-5,90,36.5,25);
  
  
  
  
  fill(144,54,59);//body2
  rect(5,60,25,5);
  rect(0,65,30,5);
  fill(105,36,41);
  rect(20,70,5,10);
  rect(25,75,5,17.5);
  
  
  
  fill(131,131,116);
  rect(10,35,20,25);
  rect(5,25,26.5,15);//lian
  rect(0,35,5,15);
  rect(5,115,10,25);//leg
  rect(20,115,7.5,25);
  
  fill(164,172,189)//sock
  rect(5,125,10,15);
  rect(20,125,7.5,15);
  
  fill(15,18,52)//shoes
  rect(20,140,7.5,5);
  rect(5,140,10,5);
  
  
  
  
  
  
  
  fill(23,27,59);
  rect(20,25,15,15);//hair
  rect(10,20,20,10);
  rect(5,20,10,20);
  rect(5,20,5,30);
  rect(-5,45,5,20);
  
  
  
  rect(-10,35,12.5,15);
  
  
  
  rect(30,35,5,20);
  rect(25,55,5,5);
  rect(25,60,3,5);
  
  rect(0,30,5,10);
  rect(10,50,5,10);
  
  pop();
  

  

  
  
  
}